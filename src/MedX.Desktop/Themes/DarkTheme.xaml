<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="#2B2B32"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="#45454E"/>

    <SolidColorBrush x:Key="Selectedbackground" Color="#1E1E24"/>
    <SolidColorBrush x:Key="SelectedBorderColor" Color="#00aada" />

    <SolidColorBrush x:Key="PrimaryForeground" Color="Snow"/>
    <SolidColorBrush x:Key="SecondaryForeground" Color="#CCCCCC"/>

    <SolidColorBrush x:Key="PrimaryBorder" Color="#703C6D"/>
    <SolidColorBrush x:Key="SecondaryBorder" Color="#00AADA"/>

</ResourceDictionary>